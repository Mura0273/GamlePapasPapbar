<Window x:Class="PapasPapbar.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="MainWindow" Height="350" Width="525">
    <Grid Margin="-2,0,2,0">
        <StackPanel Orientation="Horizontal" Height="20" VerticalAlignment="Top">
            <Button Content="Reservation" Click="Reservation_Click" Height="20" VerticalAlignment="Top"/>
            <Button Content="Medlemskab" Click="Membership_Click"/>
            <Button Content="Spil bibliotek" Click="BoardgameameLibrary_Click"/>
            <Button Content="Spil statestik" Click="GameStats_Click"/>
            <Button Content="Board" Click="Table_Click"/>
        </StackPanel>
        <Frame x:Name="Main" Margin="0,20,0,0"/>
    </Grid>
</Window>
